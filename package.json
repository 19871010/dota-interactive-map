{
  "name": "dota-interactive-map",
  "version": "4.16.0",
  "description": "Interactive Dota 2 Map",
  "main": "src/js/index.js",
  "private": true,
  "scripts": {
    "postinstall": "npm run build",
    "render": "node scripts/render.js",
    "markdown:help": "marked src/template/help.md -o src/template/help.html",
    "preprocess:html": "npm run markdown:help && node scripts/preprocess.js",
    "build": "npm run build:js && npm run build:css && npm run build:html",
    "build:js": "rollup -c --environment BUILD:development",
    "build:css": "node-sass src/scss/app.scss www/app.css",
    "build:html": "npm run render && npm run preprocess:html",
    "watch": "npm-run-all --parallel watch:*",
    "watch:js": "rollup -c -w --environment BUILD:development",
    "watch:css": "nodemon -e scss --watch src/scss -x \"npm run build:css\"",
    "watch:html": "nodemon -e j2,md --watch src -x \"npm run build:html\"",
    "dist": "mkdir -p dist && npm run dist:clean && npm run dist:js && npm run dist:css && npm run dist:html && npm run dist:copy",
    "dist:clean": "rm -rf dist/*",
    "dist:js": "rollup -c --environment BUILD:production",
    "dist:css": "npm run build:css && cleancss www/app.css --skip-rebase -o dist/app-`git rev-parse --short HEAD`.min.css",
    "dist:html": "npm run render && npm run markdown:help && node scripts/preprocess.js -- production",
    "dist:copy": "cp -r www/img dist/img && cp -r www/data dist/data",
    "rollbar:sourcemap": "gulp rollbar:sourcemap",
    "rollbar:deploy": "node scripts/rollbar_deploy.js",
    "rollbar": "npm run rollbar:sourcemap && npm run rollbar:deploy -- production",
    "deploy": "node scripts/deploy.js",
    "deploy:prod": "npm run deploy -- production && npm run rollbar"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devilesk/dota-interactive-map.git"
  },
  "author": "devilesk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/devilesk/dota-interactive-map/issues"
  },
  "homepage": "https://github.com/devilesk/dota-interactive-map#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-css": "^4.1.9",
    "del": "^3.0.0",
    "git-rev-sync": "^1.8.0",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-rollbar": "^0.1.4",
    "gulp-sourcemaps": "^2.6.2",
    "imagemin-cli": "^3.0.0",
    "jinjs": "^0.3.6",
    "js-yaml": "^3.10.0",
    "marked": "^0.4.0",
    "node-sass": "^4.6.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.3",
    "preprocess": "^3.1.0",
    "rollup": "^0.62.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-terser": "^1.0.1",
    "rollup-plugin-uglify": "^4.0.0"
  },
  "dependencies": {
    "@turf/union": "^6.0.3",
    "clean-css-cli": "^4.1.10",
    "dota-vision-simulation": "^0.17.0",
    "ol": "^5.2.0",
    "rollbar": "^2.4.4"
  }
}
