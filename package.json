{
  "name": "dota-interactive-map",
  "version": "0.2.0",
  "description": "Interactive Dota 2 Map",
  "main": "interactivemap.js",
  "scripts": {
    "watch:js": "watchify src/js/index.js --outfile www/bundle.js",
    "build:ol": "node node_modules/openlayers/tasks/build.js src/ol-custom.json www/ol-custom.js",
    "build:css" : "node-sass src/scss/app.scss www/app.css",
    "watch:css": "nodemon -e scss -x \"npm run build:css\"",
    "watch": "npm run watch:js & npm run watch:css"
  },
  "keywords": [
    "dota",
    "map"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devilesk/dota-interactive-map.git"
  },
  "author": "devilesk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devilesk/dota-interactive-map/issues"
  },
  "homepage": "https://github.com/devilesk/dota-interactive-map#readme",
  "devDependencies": {
    "@turf/bezier": "^3.7.0",
    "@turf/union": "^3.7.0",
    "browserify": "^13.1.1",
    "browserify-replace": "^0.9.0",
    "browserify-shim": "^3.8.12",
    "clean-css": "^3.4.22",
    "del": "^2.2.2",
    "dota-vision-simulation": "^0.14.4",
    "git-rev-sync": "^1.8.0",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-rollbar": "^0.1.4",
    "gulp-sourcemaps": "^1.9.1",
    "imagemin-cli": "^3.0.0",
    "node-sass": "^4.1.1",
    "nodemon": "^1.11.0",
    "ol3-popup": "0.0.1-security",
    "openlayers": "^3.20.1",
    "preprocess": "^3.1.0",
    "replace": "^0.3.0",
    "rollbar-browser": "^1.9.2",
    "uglify-js": "^2.7.5",
    "watchify": "^3.8.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "openlayers": "global:ol"
  }
}
